@import "/template/back-ground/1.wxss";

.contain {
  justify-content: center;
   padding-top: 70px; 
	position: absolute;
	 width: 100%; 
	/* height: 100%; */
}

.map {
  margin: 0 auto;
  position: relative;
  width: 90%;
  height: 350px;
  border-radius: 20px;
}

.place-info {
  position: relative;
  top: 20px;
}

.place-button {
  padding: 5px 10px;
  flex-wrap: wrap;
  align-items: center;
  justify-content: space-between;
  flex-flow: row wrap;
  height: 7em;
}

.place-button-item {
  color: #71bbb2;
  box-sizing: border-box;
   height: 50px; 
  font-size: 14px;
  padding: 10px;
  border-radius: 5px;
  background-color: rgba(255, 255, 255, 0.9);
  width: 32%;
  text-align: center;
  word-break: keep-all;
}
