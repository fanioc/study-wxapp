.view-box {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
}

input {
  padding: 5px 10px 5px 10px;
  
  height: 2em;
  background: #fff;
  opacity: 0.8;
  margin: 5px 10px 5px 10px;
  box-shadow: 10px;
  border-radius: 5px;
}

button {
  height: 10%;
  width: 100px;
}
